{
  "name": "vpp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "ios-11": "npx react-native run-ios --simulator=\"iPhone 11\"",
    "yarn-ios": "yarn && yarn pod",
    "android-rl": "npx react-native run-android --variant=release",
    "start": "npx react-native start --reset-cache",
    "clean": "cd android && ./gradlew clean && cd ..",
    "apk": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "aab": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "pod": "cd ios && pod install && cd ..",
    "lint-fix": "eslint --fix . && prettier --write .",
    "tsc": "npx tsc --noemit",
    "check-code": "yarn lint-fix && yarn lint && yarn tsc",
    "gen-styles": "cd src/lib/tailwind && npx create-tailwind-rn && cd ../../..",
    "postinstall": "patch-package",
    "eslint": "eslint --max-warnings=0 . && prettier --check .",
    "eslint-fix": "eslint --fix . && prettier --write .",
    "check-types": "npx tsc --noemit",
    "lint": "yarn eslint-fix && yarn eslint && yarn check-types",
    "codepush:key:android": "appcenter codepush deployment list -a viettel.vss.vpp-gmail.com/VPP-ANDROID -k",
    "codepush:key:ios": "appcenter codepush deployment list -a viettel.vss.vpp-gmail.com/VPP-IOS -k",
    "codepush:key": "yarn codepush:key:android && yarn codepush:key:ios",
    "codepush:release:android": "appcenter codepush release-react -a viettel.vss.vpp-gmail.com/VPP-ANDROID -d Production -m --description \"release android\"",
    "codepush:release:ios": "appcenter codepush release-react -a viettel.vss.vpp-gmail.com/VPP-IOS -d Production -m --description \"release ios\"",
    "codepush:release": "yarn codepush:release:android && yarn codepush:release:ios",
    "codepush:rollback:android": "appcenter codepush rollback -a viettel.vss.vpp-gmail.com/VPP-ANDROID Production",
    "codepush:rollback:ios": "appcenter codepush rollback -a viettel.vss.vpp-gmail.com/VPP-IOS Production",
    "codepush:rollback": "yarn codepush:rollback:android && yarn codepush:rollback:ios",
    "codepush:history:android": "appcenter codepush deployment history -a viettel.vss.vpp-gmail.com/VPP-ANDROID Production",
    "codepush:history:ios": "appcenter codepush deployment history -a viettel.vss.vpp-gmail.com/VPP-IOS Production",
    "codepush:history": "yarn codepush:history:android && yarn codepush:history:ios"
  },
